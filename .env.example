# AutoGen Programming Workflow Environment Configuration
# Copy this file to .env and fill in your API keys

# Google Gemini API Configuration (Recommended)
# Get your API key from: https://ai.google.dev/
GOOGLE_API_KEY=your_gemini_api_key_here

# OpenAI API Configuration (Alternative)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
GEMINI_MODEL=gemini-2.5-pro-preview-05-06
OPENAI_MODEL=gpt-4o-mini

# Model Parameters
TEMPERATURE=0.7
MAX_TOKENS=4000

# Workflow Configuration
MAX_ROUNDS=20
MAX_MESSAGES=50
TIMEOUT_SECONDS=300

# Logging Configuration
LOG_LEVEL=INFO

# Output Configuration
OUTPUT_DIR=workflow_output
SAVE_ARTIFACTS=true

# Development Settings
DEBUG=false
ENVIRONMENT=production

# Database Configuration (for Docker)
POSTGRES_DB=autogen_db
POSTGRES_USER=autogen_user
POSTGRES_PASSWORD=autogen_password
DATABASE_URL=postgresql://autogen_user:autogen_password@postgres:5432/autogen_db

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# Security
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# External Services
WEBHOOK_URL=
NOTIFICATION_EMAIL=

# Docker specific
COMPOSE_PROJECT_NAME=autogen-workflow
